{# src/fbeck/AdminBundle/Resources/views/Home/index.html.twig #}

{% extends "fbeckAdminBundle::layout.html.twig" %}

{% block fbeck_admin_body %}

  <h2>List of all our awesome users</h2>
  <ul>
    {% for user in users %}
		<li><a href="{{ path('fbeck_admin_view', {'id': user.id}) }}">{{ user.username }}</a></li>
	{% endfor %}
  </ul>

{% endblock %}